{
  "name": "health-anywhere-be",
  "version": "1.0.0",
  "description": "Health Anywhere is a web application that aims to help medical patients find doctors near their location. The application is written in TypeScript with ReactJs on the front-end and NodeJs with ExpressJS on the back-end, as a database it will be using PostgreSQL",
  "main": "src/server.ts",
  "repository": "https://github.com/doglez/health-anywhere-backend.git",
  "author": "Danil Gonzalez <danilo.j.gonzalez@gmail.com>",
  "license": "EULA",
  "private": true,
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.8.3",
    "yarn": ">=1.22.22"
  },
  "scripts": {
    "dev": "SET NODE_ENV=development& ts-node-dev src/server",
    "start": "SET NODE_ENV=production& node build/server",
    "test": "SET NODE_ENV=test& jest --coverage --watchAll",
    "clear_jest": "jest --clearCache",
    "imp": "SET NODE_ENV=development& ts-node src/database/seeds -i",
    "del": "SET NODE_ENV=development& ts-node src/database/seeds -d"
  },
  "dependencies": {
    "class-validator": "^0.14.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^7.4.1",
    "health-anywhere-be": "file:",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "http-status": "^1.7.4",
    "morgan": "^1.10.0",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/hpp": "^0.2.6",
    "@types/jest": "^29.5.13",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
